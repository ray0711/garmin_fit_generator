{
  "type": "module",
  "name": "garmin-workout-generator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startPublic": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch",
    "fit-sdk-generate": "npx -p typescript tsc -p ./scripts/tsconfig.fitsdk.json && find -iname profile.js",
    "fit-sdk-generate-types": "node ./scripts/generate-fit-types.mjs",
    "generate": "npm run fit-sdk-generate && npm run fit-sdk-generate-types && echo \"Generated FIT SDK types\"",
    "prebuild": "npm run generate",
    "pretest": "npm run generate",
    "lint": "ng lint",
    "prettier": "prettier  --write \"src/**/*.{ts,html,scss}\" "
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.8",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.8",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.0",
    "@garmin/fitsdk": "^21.178.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.7.1",
    "angular-eslint": "20.4.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.0"
  }
}
